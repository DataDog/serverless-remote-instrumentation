    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-yml": "^1.16.0",
    "globals": "^15.1.0",
    "jest": "^29.7.0",
    "jszip": "^3.10.1",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.1.2",
    "ts-jest": "^29.2.6",
    "typescript": "^5.9.2",
    "yaml-cfn": "^0.3.2"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "format": "eslint --fix .",
    "lint": "eslint -c eslint.config.mjs . ",
    "lint-yaml": "eslint -c eslint.config.yaml.mjs . ",
    "format-yaml": "eslint --fix -c eslint.config.yaml.mjs . ",
    "create-release": "bash scripts/create_release.sh $1",
    "test": "jest --config test/jest.config.js",
    "integ": "jest -i --config integration-tests/jest.config.js",
    "metrics-validations": "jest -i --config metrics-validations/jest.config.js",
    "integ-test-setup": "if [ -n \"$CI_PROJECT_DIR\" ]; then node ${SCRIPTS_PATH}/test_setup.js; else node scripts/test_setup.js; fi",
    "integ-test-teardown": "if [ -n \"$CI_PROJECT_DIR\" ]; then node ${SCRIPTS_PATH}/test_teardown.js; else node scripts/test_teardown.js; fi"
  }
