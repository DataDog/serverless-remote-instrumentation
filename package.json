    "@aws-sdk/client-s3": "^3.858.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.858.0",
    "@aws-sdk/client-secrets-manager": "^3.858.0",
    "@aws-sdk/s3-request-presigner": "^3.858.0",
    "@datadog/datadog-api-client": "^1.40.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "aws-cdk": "^2.1023.0",
    "aws-cdk-lib": "^2.208.0",
    "cfn-response": "^1.0.1",
    "constructs": "^10.4.2",
